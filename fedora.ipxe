#!ipxe
goto menu_shit

:menu_shit
set os fedora
menu fedora
item fwstx86 Fedora Workstation x86
item fsrvx86 Fedora Server x86
item fwstx86_64 Fedora Workstation x86_64
item fsrvx86_64 Fedora Server x86_64
choose version || goto fedora_exit
goto ${version}

:fwstx86
kernel http://mirror.rit.edu/fedora/fedora/linux/releases/23/Workstation/i386/os/images/pxeboot/vmlinuz-PAE repo=http://mirror.rit.edu/fedora/fedora/linux/releases/23/Workstation/i386/os/
initrd http://mirror.rit.edu/fedora/fedora/linux/releases/23/Workstation/i386/os/images/pxeboot/initrd-PAE.img
boot

:fwstx86_64
kernel http://mirror.rit.edu/fedora/fedora/linux/releases/23/Workstation/x86_64/os/images/pxeboot/vmlinuz repo=http://mirror.rit.edu/fedora/fedora/linux/releases/23/Workstation/x86_64/os/
initrd http://mirror.rit.edu/fedora/fedora/linux/releases/23/Workstation/x86_64/os/images/pxeboot/initrd.img
boot

:fsrvx86
kernel http://mirror.rit.edu/fedora/fedora/linux/releases/23/Server/i386/os/images/pxeboot/vmlinuz-PAE repo=http://mirror.rit.edu/fedora/fedora/linux/releases/23/Server/i386/os/
initrd http://mirror.rit.edu/fedora/fedora/linux/releases/23/Server/i386/os/images/pxeboot/initrd-PAE.img
boot

:fsrvx86_64
kernel http://mirror.rit.edu/fedora/fedora/linux/releases/23/Server/x86_64/os/images/pxeboot/vmlinuz repo=http://mirror.rit.edu/fedora/fedora/linux/releases/23/Server/x86_64/os/
initrd http://mirror.rit.edu/fedora/fedora/linux/releases/23/Server/x86_64/os/images/pxeboot/initrd.img
boot

:fedora_exit
clear menu
clear kernel
clear initrd


#!ipxe

set cent iscsi:192.168.200.145::::iqn.2016-05.org.freenas.ctl:NoneTarget
set deb iscsi:192.168.200.145::::iqn.2016-05.org.freenas.ctl:booptarget

menu sanboot
item --gap --             --------------------------- OS -----------------------------------
item debain8 Debian 8
item centos6 Centos 6
item --gap --             ------------------------- Options --------------------------------
item goback Go back to previous menu
choose version || goto exit_this_crap
goto ${version}

:debian8
sanboot ${deb}

:centos6
sanboot ${cent}

goback
chain http://adriankoshka.xyz/boot/linux.ipxe

:exit_this_crap
clear menu
exit

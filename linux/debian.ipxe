#!ipxe

goto debian

:debian
#console --picture http://boot.ipxe.org/ipxe.png
set os debian
menu debian
item --gap --             -------------------------- Arch ----------------------------------
item Debian8_x86 Debian 8 32-bit net install
item Debian8_x86_64 Debian 8 64-bit net install
item --gap --             ------------------------- Options --------------------------------
item goback Go back to previous menu
choose version || goto debian_exit
goto ${version}

:Debian8_x86
echo Booting Debian 8 32-bit
kernel http://mirror.rit.edu/debian/dists/jessie/main/installer-i386/current/images/netboot/debian-installer/i386/linux
initrd http://mirror.rit.edu/debian/dists/jessie/main/installer-i386/current/images/netboot/debian-installer/i386/initrd.gz
boot

:Debian8_x86_64
echo Booting Debian 8 64-bit
kernel http://mirror.rit.edu/debian/dists/jessie/main/installer-amd64/current/images/netboot/debian-installer/amd64/linux
initrd http://mirror.rit.edu/debian/dists/jessie/main/installer-amd64/current/images/netboot/debian-installer/amd64/initrd.gz
boot

:goback
chain http://adriankoshka.xyz/boot/linux.ipxe

:debian_exit
clear menu

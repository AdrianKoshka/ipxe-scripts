#!ipxe

goto windows

:windows
set server_ip 10.0.0.9
set http_path /win7
console --picture http://boot.ipxe.org/ipxe.png
set os windows7
menu Windows 7
item Win7HP64 Windows 7 Home Premium 64-bit
#item Debian8_x86_64 Debian 8 64-bit net install
choose version || goto windows_exit
goto ${version}


:Win7HP64
kernel wimboot
initrd bootmgr
initrd boot/bcd BCD
initrd boot/boot.sdi boot.sdi
initrd sources/boot.wim boot.wim
boot

:windows_exit
clear menu
